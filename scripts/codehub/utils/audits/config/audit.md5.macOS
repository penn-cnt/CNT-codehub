find "INDIR_SUBSTR" -maxdepth 1 -type f -not -path '*._*' -not -path '*~$*' -exec sh -c 'echo -n "{} " && md5 "{}" | cut -d" " -f1 && stat -c "%s" "{}" | awk "{printf \"%.2f MB\n\", \$1/1024/1024}" && stat -c "%y" "{}" | cut -d" " -f1 && stat -c "%U" "{}"' \; > "OUTDIR_SUBSTR"
find "INDIR_SUBSTR" -maxdepth 1 -type f -not -path '*._*' -not -path '*~$*' -exec sh -c 'echo -n "{} " && md5 "{}" | awk \'/Size:/{printf "%.2f MB ", $2/1024/1024} /Modify:/{print $2}\' && ls -l "{}" | awk \'{print $3}\'' \;
find . -type f -print0 | xargs -0 -I{} sh -c 'md5 -q "{}"; ls -l "{}" | awk "{print \$3}"; date -r "{}" "+%Y-%m-%d"'
